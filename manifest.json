{
    "manifest_version": 3,
    "name": "YT AD Skipper",
    "version": "3.25",
    "description": "YTSkiper is cooooooool",
    "icons": {
        "128": "icon/suzume-128.png"
    },
    "action": {},
    "content_scripts": [
        {
            "matches": [
                "*://*.youtube.com/*"
            ],
            "js": [
                "content-script.js"
            ],
            "run_at": "document_idle",
            "all_frames": false
        }
    ],
    "permissions": [
        "scripting"
    ],
    "host_permissions": [
        "*://*.youtube.com/*"
    ]
}